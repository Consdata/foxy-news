{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "foxy-news-web": {
      "root": "apps/foxy-news-web/",
      "sourceRoot": "apps/foxy-news-web/src",
      "projectType": "application",
      "prefix": "fxy",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/foxy-news-web",
            "index": "apps/foxy-news-web/src/index.html",
            "main": "apps/foxy-news-web/src/main.ts",
            "polyfills": "apps/foxy-news-web/src/polyfills.ts",
            "tsConfig": "apps/foxy-news-web/tsconfig.app.json",
            "assets": [
              "apps/foxy-news-web/src/favicon.ico",
              "apps/foxy-news-web/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/foxy-news-web/src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/foxy-news-web/src/environments/environment.ts",
                  "with": "apps/foxy-news-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "foxy-news-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "foxy-news-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "foxy-news-webb:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/foxy-news-web/tsconfig.app.json",
              "apps/foxy-news-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/foxy-news-web/jest.config.js",
            "tsConfig": "apps/foxy-news-web/tsconfig.spec.json",
            "setupFile": "apps/foxy-news-web/src/test-setup.ts"
          }
        }
      }
    },
    "foxy-news-web-e2e": {
      "root": "apps/foxy-news-web-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
